<!DOCTYPE html>
<html>
  <head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Labels">
    <meta name="author" content="Alexandra Dohm">
    <meta charset="utf-8">

    <title>Underground Labels</title>
<!--     <link rel="stylesheet" href="/Animals/animals.css" type="text/css" />  -->
  </head>

  <body>     
  <!-- Load navagation bar-->  
  {{> nav }} 

  <!-- Table that displays all animals -->
  <table id = "aTable">
  <tbody> 
    {{#each label}} 
    <tr> 
      <td>{{name}}</td> 
      <td>{{country}}</td> 
      <td>{{bandcamp}}</td> 
      <td>{{soundcloud}}</td> 
      <td>{{facebook}}</td> 
    </tr> 
    {{/each}} 
  </tbody> 
  </table> 


<!--  <script type="text/javascript" src="/Animals/animals.js"></script>
   <script type="text/javascript" src="/jquery.js"></script>  -->

  </body>
</html>